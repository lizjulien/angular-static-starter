{
  "name": "angular-static-starter",
  "version": "0.0.1",
  "description": "Angular Static Starter give you the power of Angular to generate static website",
  "homepage": "https://github.com/lizjulien/angular-static-starter",
  "license": "MIT",
  "author": "Moulin Julien <julien@baliz.org> (http://baliz.org/)",
  "scripts": {
    "start": "ng serve",
    "build": "run-s server:stop build:client build:aot build:server server:start create:static server:stop",
    "build:client": "ng build -prod --build-optimizer --app 0",
    "build:aot": "ng build --aot --app 1",
    "build:server": "webpack -p",
    "server:start": "node_modules/pm2/bin/pm2 start launchServer.sh --name buildServer -f",
    "server:stop": "node_modules/pm2/bin/pm2 delete -s buildServer || :",
    "create:static": "run-s remove:static build:static",
    "remove:static": "rm -Rf site && sleep 10",
    "build:static": "node build-static"
  },
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.1",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@angular/platform-server": "^5.1.2",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "autoprefixer": "^6.5.3",
    "circular-dependency-plugin": "^4.2.1",
    "codelyzer": "^4.0.1",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "exports-loader": "^0.6.3",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.29.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less-loader": "^4.0.5",
    "npm-run-all": "^4.1.2",
    "pm2": "^2.9.1",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.1.2",
    "protractor": "~5.1.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.13.1",
    "stylus-loader": "^3.0.1",
    "ts-loader": "^3.2.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "uglifyjs-webpack-plugin": "~1.1.2",
    "url-loader": "^0.6.2",
    "webpack": "~3.10.0",
    "webpack-dev-server": "~2.9.3",
    "webpack-node-externals": "^1.6.0",
    "website-scraper": "^3.3.2"
  }
}
